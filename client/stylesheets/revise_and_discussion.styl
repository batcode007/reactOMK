/* Main Container */

.header_wrapper {
    &.student_library {
        padding: 15px 15px 35px 15px!important;
        border-bottom: 2px solid #ddd;
        .search_wrapper_milestone {
            #search_filter {
                float:left;
                display:inline-block;
                margin-right:30px;
                button {
                    background: transparent;
                    border: 2px solid #ddd;
                    width: 200px;
                    text-align: left;
                    padding: 5px 0 5px 10px;
                    color: #6e6e6e;
                    text-decoration: none;
                    display: block;
                    font-weight: 300;
                    line-height: 18px;
                    white-space: nowrap;
                    margin-top: -6px;
                    i {
                        float:right;
                        margin-right:10px;
                        color:#d9d9d9;
                    }
                }
                .dropdown-menu {
                    min-width: 200px;
                }
            }
        }
    }
    background: #fff;
    .breadcrumb_list {
        float: left;
        display: inline-block;
        @media (max-width: 767px) {
            float: none;
            display: block;
        }
        .iconLink {
            display: inline-block;
            margin-right: 40px;
            position: relative;
            z-index: 9;
            &:after {
                content: '';
                background: #e0e0e0;
                width: 1px;
                height: 40px;
                position: absolute;
                right: -20px;
                top: -10px;
            }
        }
        ul {
            padding: 0;
            list-style: none;
            display: inline-block;
            li {
                font-size: 14px;
                color: rgba(0, 0, 0, .54);
                font-weight: 700;
                display: inline-block;
                margin-right: 15px;
                position: relative;
                &:after {
                    content: '';
                    background: url('../images/right_arrow.png') no-repeat;
                    width: 5px;
                    height: 8px;
                    position: absolute;
                    top: 7px;
                    right: -12px;
                }
                &:last-child {
                    margin-right: 0;
                    &:after {
                        background: transparent;
                    }
                }
                a {
                    color: rgba(0, 0, 0, .87);
                }
            }
        }
    }
    .posted_on_info {
        float: right;
        margin-top: -12px;
        @media (max-width: 767px) {
            float: none;
            margin-top: 0;
        }
        ul {
            padding: 0;
            list-style: none;
            li {
                display: inline-block;
                margin-right: 20px;
                @media (max-width: 800px) {
                    margin-right: 10px;
                }
                &:last-child {
                    margin-right: 0;
                }
                &.attempt_resources {
                    position: relative;
                    top: 10px;
                    margin-left: 10px;
                    @media (max-width: 800px) {
                        margin-right: 0;
                    }
                    a {
                        font-size: 14px;
                        color: #444444;
                        text-transform: uppercase;
                        font-weight: 400;
                        background: #ffd75e;
                        letter-spacing: .5px;
                        padding: 10px 15px;
                        -webkit-transition: all .5s ease;
                        -moz-transition: all .5s ease;
                        -ms-transition: all .5s ease;
                        -o-transition: all .5s ease;
                        transition: all .5s ease;
                        position:relative;
                        top:-2px;
                        @media (max-width: 480px) {
                            padding: 12px 18px;
                            font-size: 12px;
                        }
                        &:hover {
                            -webkit-transition: all .5s ease;
                            -moz-transition: all .5s ease;
                            -ms-transition: all .5s ease;
                            -o-transition: all .5s ease;
                            transition: all .5s ease;
                            background: #ffd75e;
                        }
                    }
                }
                a {
                    text-decoration: none;
                    .like {
                        width: 35px;
                        height: 35px;
                        border-radius: 50%;
                        text-align: center;
                        border: 1px solid #f7a229;
                        display: inline-block;
                        -webkit-transition: all .12s ease;
                        -moz-transition: all .12s ease;
                        -ms-transition: all .12s ease;
                        -o-transition: all .12s ease;
                        transition: all .12s ease;
                        &:hover {
                            -webkit-transition: all .12s ease;
                            -moz-transition: all .12s ease;
                            -ms-transition: all .12s ease;
                            -o-transition: all .12s ease;
                            transition: all .12s ease;
                            -webkit-transform: scale(.9);
                            -moz-transform: scale(.9);
                            -ms-transform: scale(.9);
                            -o-transform: scale(.9);
                            transform: scale(.9);
                        }
                        img {
                            position: relative;
                            top: 9px;
                        }
                    }
                    span {
                        font-size: 14px;
                        color: rgba(0, 0, 0, .7);
                        font-weight: 500;
                        position: relative;
                        top: 10px;
                        left: 5px;
                    }
                }
            }
        }
    }
}

