.checkbox {
    margin: 0;
    padding: 0;
    float: left;
    margin-left: -10px;
    margin-right: 10px
}

.checkbox input[type=checkbox] {
    display: none;
}

.checkbox label {
    min-height: 0;
    padding-left: 15px!important;
}

.checkbox label:before {
    content: "";
    display: inline-block;
    width: 15px;
    height: 15px;
    position: absolute;
    left: 22px;
    top: 2px;
    border:2px solid #b2b0a7;
}

.checkbox input[type=checkbox]:checked + label:before {
    content: "\2713";
    font-size: 14px;
    color: #7f7f7f;
    text-align: center;
    line-height: 12px;
    font-weight: 700;
}

.sidebar_tree_wrapper {
    font-family: "Roboto", sans-serif;
    .overlay {
        position: fixed;
        width: 100%;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        background: rgba(170, 170, 170, .6);
        transition: width .1s ease;
        display: none;
        &.open {
            display: block;
        }
    }
    .sidebar_tree_container {
        font-family: "Roboto", sans-serif;
        position: fixed;
        width: 30%;
        height: 100%;
        top: 41px;
        overflow-y: hidden;
        left: 0;
        right: 0;
        bottom: 0;
        background: #fff;
        box-shadow: 1px 0px 10px 0px rgba(0, 0, 0, 0.14);
        transform: translateX(-100%);
        transition: transform .5s cubic-bezier(.17, .67, .22, .91);
        &.open {
            transform: translateX(0);
            transition: transform .5s cubic-bezier(.17, .67, .22, .91);
        }
        .course_info_wrap {
            background: #b549c6;
            padding: 12px 20px 15px 20px;
            margin-top: 50px;
            h3 {
                font-size: 15px;
                color: #fff;
                line-height: 18px;
                font-weight: 500;
                margin: 0;
                margin-bottom: 10px;
            }
            ul {
                padding: 0;
                margin: 0;
                li {
                    display: inline-block;
                    margin-right: 30px;
                    &:last-child {
                        margin-right: 0;
                    }
                    a {
                    	text-decoration: none;
                        span {
                            width: 30px;
                            height: 30px;
                            display: inline-block;
                            background: #fff;
                            border-radius: 50%;
                            text-align: center;
                            i {
                                color: #f7a025;
                                font-size: 17px;
                                position: relative;
                                top: 6px;
                            }
                        }
                        b {
                            font-size: 15px;
                            color: #fff;
                            line-height: 18px;
                            font-weight: 400;
                            display: inline-block;
                            position: relative;
                            top: 5px;
                            left: 5px;
                        }
                    }
                }
            }
        }
        .milestone_tree_list_wrap {
            margin:5px;
        	overflow-y:auto;
            &::-webkit-scrollbar{
                width: 8px;
            }
            &::-webkit-scrollbar-track{
                background:transparent;
            }

            &::-webkit-scrollbar-thumb{
              background:#ddd;
              border-radius:0;
            }
        }
    }
}

