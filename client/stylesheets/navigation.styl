a {
  text-decoration : none!important;
}
.navbar-default {
  .navbar-right {
    li {
      &.notification_wrapper {
          font-family: "Roboto", sans-serif;
        .notication_arrow {
          display: block;
          width: 0;
          height: 0;
          vertical-align: middle;
          border-bottom: 8px dashed #BBBBBB;
          border-top: 4px solid\9;
          border-right: 8px solid transparent;
          border-left: 8px solid transparent;
          position: absolute;
          right: 25px;
          top: -8px;
        }
        .notification_icon {
          position: relative;
          &:hover {
            color:#B0FE76;
          }
          i {
            font-size: 20px;
            position: relative;
            top: 2px;
          }
          span {
              width: 16px;
              height: 16px;
              background: #ff0000;
              border-radius: 50%;
              text-align: center;
              position: absolute;
              top: 6px;
              right: 10px;
            b {
              font-size: 10px;
              text-transform: uppercase;
              font-family: "Roboto", sans-serif;
              position: relative;
              top: -3px;
              color: #fff;
            }
          }
        }
        .dropdown-menu {
            right: 0 !important;
            width: 310px !important;
            background: #bbb;
            font-family: "Roboto", sans-serif;
            border-radius: 4px!important;
            margin-top: 5px;
          .header_wrap {
            span {
              &.title {
                text-align: center;
                display: block;
                font-size: 15px;
                line-height: 18px;
                color: #000;
                font-weight: 500;
                background: #bbb;
                text-transform:uppercase;
                padding: 5px 0;
                border-radius: 4px;
              }
            }
            i {
              &.notification_close {
                color: rgba(255, 255, 255, 0.54);
                position: absolute;
                right: 5px;
                top: 3px;
                font-size: 18px;
                cursor: pointer;
              }
            }
          }
          .notification_list_wrap {
              max-height: 445px;
              overflow-y: auto;
              &::-webkit-scrollbar {
                  width: 5px;
              }

              &::-webkit-scrollbar-track {
                  background: transparent;
              }

              &::-webkit-scrollbar-thumb {
                  border-radius: 10px;
                  background: #A0A0A0;
              }
               li {
                  border-bottom: 1px solid #d0d0d0!important;
                  padding: 5px 20px 5px 5px;
                  position:relative;
                  border-radius: 4px;
                  margin: 4px;
                  -webkit-box-shadow: 0px 1px 5px 0px rgba(0,0,0,0.35);
                  -moz-box-shadow: 0px 1px 5px 0px rgba(0,0,0,0.35);
                  box-shadow: 0px 1px 5px 0px rgba(0,0,0,0.35);
                  &:last-child {
                    margin-bottom:0;
                  }
                  &.unread {
                    background:#efefef;
                    a {
                      font-weight: 700;
                      color: #010101;
                    }
                  }
                  &.read {
                    background:#fff;
                    a {
                      font-weight: 400;
                      color: #010101;
                    }
                  }
                  .delete_notification {
                    position: absolute;
                    top: 2px;
                    right: 4px;
                    cursor:pointer;
                  }
                  .mark_featured {
                    position: absolute;
                    top: 0;
                    z-index: 1;
                    left: 0;
                  }
                  .notify_icon {
                    position: absolute;
                  }
                  .notify_content {
                    margin-left:50px;
                  }
                  a {
                      white-space: normal!important;
                      font-size: 12px;
                      line-height: 14px!important;
                      background:transparent;
                      min-height: 45px;
                      display: block;
                      text-transform: initial;
                  }
                  .posted_on {
                    span {
                      color: #9c9c9c;
                      font-size: 10px;
                      font-family: "Roboto", sans-serif;
                      font-weight: 500;
                      &.ignore_wrap {
                        float: right;
                      }
                      a {
                        &.ignore_btn{
                            color: rgba(255, 255, 255, 0.54);
                            font-size: 13px;
                            text-transform: uppercase;
                            font-family: "Roboto", sans-serif;
                            font-weight: 500;
                            text-decoration: underline!important;
                        }
                      }
                    }
                  }
                }
            }

          .footer_wrap {
            height: 24px;
            position: relative;
            span {
                display: block;
                text-align: center;
                color: #000;
                font-size: 13px;
                text-transform: uppercase;
                font-family: "Roboto", sans-serif;
                font-weight: 500;
                position: relative;
                top: 4px;
                z-index: -1;
            }
            .previous_wrap {
              &.disabled {
                opacity:.4;
                a {
                  cursor:default;
                }
              }
              &.previous {
                  position: absolute;
                  left: 5px;
                  top: 1px;
                  -webkit-transform: rotate(180deg);
                  -moz-transform: rotate(180deg);
                  -ms-transform: rotate(180deg);
                  transform: rotate(180deg);
                  cursor:pointer;
              }
               &.next {
                  position: absolute;
                  right: 5px;
                  top: 0;
                  cursor:pointer;
              }
            }
          }
        }
      }
    }
  }
}

.mce-path-item {
  display:none!important;
}