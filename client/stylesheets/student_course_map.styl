    .rating-progress {
        width: 100px;
        height: 100px;
        position: relative;
        background-color: #ebebeb;
        border-radius: 50%;
        margin: 0 auto;
    }
    .rating-progress .circle .mask,
    .rating-progress .circle .fill,
    .rating-progress .circle .shadow {
        width: 100px;
        height: 100px;
        position: absolute;
        border-radius: 50%;
    }
    .rating-progress .circle .mask,
    .rating-progress .circle .fill {
        -webkit-backface-visibility: hidden;
        transition: -webkit-transform 1s;
        transition: -ms-transform 1s;
        transition: transform 1s;
    }
    .rating-progress .circle .mask.reanimate,
    .rating-progress .circle .fill.reanimate {
        transition: -webkit-transform 0s;
        transition: -ms-transform 0s;
        transition: transform 0s;
    }
    .rating-progress .circle .mask.reset,
    .rating-progress .circle .fill.reset {
        -webkit-transform: rotate(0deg) !important;
        -ms-transform: rotate(0deg) !important;
        transform: rotate(0deg) !important;
    }
    .rating-progress .circle .mask {
        clip: rect(0px, 100px, 100px, 50px);
    }
    .rating-progress .circle .mask .fill {
        clip: rect(0px, 50px, 100px, 0px);
        background: #257a9f;
    }
    .locked_milestone .rating-progress .circle .mask .fill {
        clip: rect(0px, 50px, 100px, 0px);
        background: #EBEBEB;
    }
    .skipped_milestone .rating-progress .circle .mask .fill {
        clip: rect(0px, 50px, 100px, 0px);
        background: #EBEBEB!important;
    }
    .rating-progress .inset {
        width: 92px !important;
        height: 92px !important;
        position: absolute;
        top: 50%;
        left: 50%;
        margin: -46px 0 0 -46px !important;
        background-color: #fff;
        border-radius: 50%;
        text-align: center;
