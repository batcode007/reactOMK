.skip_milestone {
    &.hidden_milestone {
        position:relative;
        right:20px;
        top:20px;
        .dropdown-menu {
            left: auto;
            right: 0;
        }
    }
    .btn-skip {
        position:absolute;
        width:35px;
        height:35px;
        background:#eee;
        right:0;
        border:none;
        border-radius:50%;
        display:none;
        i {
            color: #9E9E9E;
            text-align: center;
            position: relative;
            top: 1px;
        }
        span {
                width: 5px;
                height: 5px;
                display: block;
                background: #ccc;
                border-radius: 50%;
                text-align: center;
                margin: 4px auto;
        }
    }
    .dropdown-menu {
        left: auto;
        right: -125px;
        a {
            display:block!important;
        }
    }
}


.milestone_list_wrap, .sidebar, .description_section {
     overflow-y:auto;
     overflow-x:hidden;
    &::-webkit-scrollbar{
        width: 8px;
    }

    &::-webkit-scrollbar-track{
        background:transparent;
    }

    &::-webkit-scrollbar-thumb{
      background:#ddd;
      border-radius:20px;
    }
    &::-moz-scrollbar{
        width: 8px;
    }

    &::-moz-scrollbar-track{
        background:transparent;
    }

    &::-moz-scrollbar-thumb{
      background:#ddd;
      border-radius:20px;
    }

}
